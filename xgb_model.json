import pickle
import xgboost as xgb

# Load the model using pickle (assuming it was initially saved this way)
with open('xgb_model.pkl', 'rb') as f:
    xgboost_model = pickle.load(f)

# Save the model using XGBoost's save_model method to create xgb_model.json
xgboost_model.save_model('xgb_model.json')

